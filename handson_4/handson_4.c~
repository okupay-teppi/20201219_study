/* bof.c */
#include <stdio.h>
#include <stdlib.h>
#include <string.h>


void vul_1()
{
  char buf1[30] = {};  /* set all bytes to zero */
  
  printf("buf1 = %p\n", buf1);
  printf("puts = %p\n", puts);
  printf("system = %p\n", system);
  
  gets(buf1);
  puts(buf1);
}

int main(int argc, char *argv[])
{
  vul_1();	
  return 0;
}
